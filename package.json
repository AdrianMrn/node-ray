{
  "name": "ray-node",
  "version": "1.0.0",
  "description": "package for working with the Ray debugger app in nodejs",
  "main": "dist/index.js",
  "scripts": {
    "test": "./node_modules/.bin/jest tests",
    "fmt": "./node_modules/.bin/prettier --config .prettierrc --write $(find src/ tests/ -printf '%p ' -type f -regex '.*\\.([tj]s|json)$')",
    "lint": "./node_modules/.bin/eslint --ext ts,js src/",
    "lint:fix": "./node_modules/.bin/eslint --ext ts,js --fix src/",
    "fix": "npm run fmt && npm run lint:fix",
    "build:dev": "./node_modules/.bin/esbuild --platform=node --target=node14 --define:__APP_VERSION__=\"'1.0'\" --format=cjs --outdir=dist src/index.ts",
    "build:prod": "./node_modules/.bin/esbuild --bundle --platform=node --target=node14 --define:__APP_VERSION__=\"'1.0'\" --format=cjs --outdir=build --minify --external:axios src/Ray.ts",
    "dev": "npm run build:dev && node dist/index.js"
  },
  "author": "",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "npm run fmt",
      "npm run lint"
    ]
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/md5": "^2.2.1",
    "@types/node": "^14.14.20",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "esbuild": "^0.8.21",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.2.0",
    "husky": "^4.3.7",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "ts-mixer": "^5.4.0",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "md5": "^2.3.0",
    "uuid": "^8.3.2"
  }
}
